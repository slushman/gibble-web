{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@components/*": ["src/components/*.astro"],
      "@layouts/*": ["src/layouts/*.astro"],
      "@types": ["src/types"],
      "@utils": ["src/utils"],
    },
  },
}
